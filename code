import java.util.*;
public class B {
	public static void main(String[] args) {
		String a,b;
		
		Scanner scan = new Scanner(System.in);
		
		a=scan.nextLine();
		b=scan.nextLine();
		
		int x=Integer.parseInt(a,2);
		int y=Integer.parseInt(b,2);
		
		System.out.println(Integer.toBinaryString(x+y));
		
		scan.close();
	}
	

}
